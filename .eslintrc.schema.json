{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "env": {
    "shared-node-browser": true,
    "browser": true,
    "es6": true,
    "commonjs": true
  },
  "rules": {
    "import/no-unresolved": "off",
    "import/extensions": "off",
    "import/no-extraneous-dependencies": "off",
    "func-names": "off",
    "linebreak-style": "off",
    "max-len": [
      "error",
      200
    ],
    "object-curly-newline": [
      "error", {
        "multiline": true
      }
    ]
  }
}
